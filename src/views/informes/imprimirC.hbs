                    <input type="checkbox" name="cbox29" id="cbox29"> <label for="cbox29">Adenocarcinoma</label><br>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <input type="checkbox" name="cbox30" id="cbox30"> <label for="cbox30">Endocervical</label><br>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <input type="checkbox" name="cbox31" id="cbox31"> <label for="cbox31">Endometrial</label><br>
