<div class="container">
    <table border="0">
        <thead>
            <tr class="header-space">
                <td colspan="3">
                    <div>
                        &nbsp;
                    </div>
                </td>
            </tr>
        </thead>
        <tbody>
            <!--Linea Horizontal-->
            <tr>
                <td colspan="4" style="color: rgb(117, 117, 117); font-size: 22px; line-height: 0px; padding-left: 0px; padding-bottom: 0px; padding-top: 0px; width:1000px;">
                    <strong><i>___________________________________________________________________________________</i></strong>
                </td>
            </tr>
            <!--Cuerpo Inf Citologias 
                    Calidad de muestra-->
            <tr>
                <td class="titulo" colspan="3" style="padding-top: 20px;">
                    <p class="titulo"><strong>CALIDAD DE MUESTRA</strong></p>
                </td>
            </tr>
            <tr>
                <label class="form-check-label">
                    <td style="width:360px;">
                        <input style="" type="radio" name="cal_muestra"
                            id="cal_muestra0" value="0">&nbsp;Satisfactoria <br>(Con celulas endocervicales / Z. T.)
                    </td>
                    <td style="width:360px;">
                        &nbsp;&nbsp;&nbsp;&nbsp;<input class="form-check-input" type="radio" name="cal_muestra"
                            id="cal_muestra1" value="1"> Satisfactoria <br>(Sin celulas endocervicales / Z. T.)
                    </td>
                    <td style="width:200;">
                        &nbsp;&nbsp;&nbsp;&nbsp;<input class="form-check-input" type="radio" name="cal_muestra"
                            id="cal_muestra2" value="2">Insatisfactoria por: <br>{{informe.ins_motivo}}
                    </td>
                </label>
                <script>
                    document.getElementById("cal_muestra{{informe.cal_muestra}}").checked = true;
                </script>

            </tr>
        </tbody>
    </table>

    <table>
        <!--Categoria General-->
        <tr>
            <td class="titulo" colspan="3">
                <p class="titulo"><strong>CATEGORIZACION GENERAL</strong></p>
            </td>
        </tr>
        <tr>
            <div class="form-check form-check-inline">
                <label class="form-check-label">
                    <td style="width:430px;" scope="row">
                        &nbsp;&nbsp;&nbsp;&nbsp;<input class="form-check-input" type="radio" name="cat_gen"
                            id="cat_gen0" value="0"> Negativa
                        para lesión intraepitelial o malignidad
                    </td>
                    <td style="width:400px;">
                        &nbsp;&nbsp;&nbsp;&nbsp;<input class="form-check-input" type="radio" name="cat_gen"
                            id="cat_gen1" value="1">
                        Anormalidad en células epiteliales
                    </td>
                    <td style="width:100px;" colspan="2">
                        &nbsp;&nbsp;&nbsp;&nbsp;<input class="form-check-input" type="radio" name="cat_gen"
                            id="cat_gen2" value="2"> Otros
                    </td>
                </label>
            </div>
        </tr>
    </table>
    <br>
    <table border="0">
        <tbody>
            <tr>
                <!--microorganizmos -izq-->
                <td style="width:500px;">
                    <p class="titulo-alt"><strong>MICROORGANISMOS</strong></p>
                    <input type="checkbox" name="cbox1" id="cbox1"><label for="cbox1">Tricomoma vaginalis</label><br>
                    <input type="checkbox" name="cbox2" id="cbox2"> <label for="cbox2">Hongos consistentes con
                        Candida
                        sp</label><br>
                    <input type="checkbox" name="cbox3" id="cbox3"> <label for="cbox3">Bacterias consistentes con
                        Actinomyces sp</label><br>
                    <input type="checkbox" name="cbox4" id="cbox4"> <label for="cbox4">Efectos citopáticos por virus
                        Herpes simple</label><br>
                    <input type="checkbox" name="cbox5" id="cbox5"> <label for="cbox5">Alteraciones en flora
                        sugestivos de vaginosis bacteriana</label><br>
                    <input type="checkbox" name="cbox6" id="cbox6"> <label for="cbox6">Oros</label>
                </td>
                <!--Otros Hallazgos-->
                <td style="width:500px;">
                    <p class="titulo2"><strong>OTROS HALLAZGOS NO NEOPLASTICOS</strong></p>
                    <input type="checkbox" name="cbox7" id="cbox7"> <label for="cbox7">Cambios Célulares reactivos
                        asociados</label><br>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <input type="checkbox" name="cbox8" id="cbox8"> <label for="cbox8">Inflamación</label>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <input type="checkbox" name="cbox9" id="cbox9"> <label for="cbox9">Atrofia</label><br>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <input type="checkbox" name="cbox10" id="cbox10"> <label for="cbox10">Radiación</label>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    <input type="checkbox" name="cbox11" id="cbox11"> <label for="cbox11">DIU</label><br>
                    <input type="checkbox" name="cbox12" id="cbox12"> <label for="cbox12">Endometriales despues de
                        40
                        Años de edad</label><br>
                    <input type="checkbox" name="cbox13" id="cbox13"> <label for="cbox13">Otros</label><br>
                    &nbsp;
                </td>
            </tr>
        </tbody>
    </table>
    <table border="0">
        <tbody>
            <tr>
                <td class="titulo" colspan="3" style="padding-top: 20px;">
                    <p><strong>ANORMALIDADES DE CELULAS EPITELIALES</strong></p>
                </td>
            </tr>
            <tr>
                <td style="width:350px;">
                    <input type="checkbox" name="cbox14" id="cbox14"> <label for="cbox14">Celulas Escamosas Atipicas
