                    <input type="checkbox" name="cbox32" id="cbox32"> <label for="cbox32">Extrauterino</label><br>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <input type="checkbox" name="cbox33" id="cbox33"> <label for="cbox33">No
                        especificado</label><br>
                </td>
            </tr>
            <tr>
                <td style="width:1000px;" colspan="3" style="padding-top: 100px;">
                    <br>
                    <strong class="titulo">OBSERVACIONES</strong>
                    <pre>{{informe.observaciones}}</pre>
                </td>
            </tr>
            <tr>
                <td colspan="3" style="vertical-align: bottom;width:550px;">&nbsp;</td>
            </tr>
            <tr>
